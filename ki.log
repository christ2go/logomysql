	; Versuche einzelne Worte wiederzufinden => mindestens ein Wort
	setze "neuedaten []
	foreach :daten[
		localmake "test ?
		if (countelements :frage :test) > 1[ setze "neuedaten ml :test :neuedaten]
	]

	; "Suchergebnisse gefunden"
	;Objektsuche
	if :objecttype = "date[
		getdates :neuedaten
		; Mehrmals vorkommende Daten finden
		
	]
ende

pr countelements :satz :elemente
	setze "uebereinstimmung 0
	foreach :satz[
		localmake "oldtest ?
		foreach :elemente[
			localmake "newtest ?
			;dz satz :oldtest :newtest
			if :oldtest = :newtest[setze "uebereinstimmung :uebereinstimmung +1 ]
		]
	]
	rg :uebereinstimmung
ende

